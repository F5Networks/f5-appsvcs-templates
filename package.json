{
  "name": "f5-appsvcs-templates",
  "version": "1.9.0-rc.3",
  "author": "F5 Networks",
  "license": "Apache-2.0",
  "description": "",
  "repository": "github:F5Networks/f5-appsvcs-templates",
  "main": "nodejs/templateWorker.js",
  "scripts": {
    "lint": "eslint nodejs lib presentation/app.js test scripts stand-alone",
    "test": "npm run testunit",
    "testunit": "mocha --recursive \"./test/unit/*.js\"",
    "testinteg": "mocha --recursive \"./test/integ/*.js\"",
    "testtemplates": "mocha --recursive \"./test/templates/*.js\"",
    "testall": "npm run lint && npm run testunit && npm run testtemplates",
    "coverage": "nyc npm test",
    "buildgui": "./scripts/build-presentation-layer.sh",
    "buildrpm": "./scripts/rpmbuild.sh",
    "build": "npm run buildgui && npm run buildrpm",
    "version": "./scripts/fix-version.sh",
    "start": "node --unhandled-rejections=strict stand-alone/app.js",
    "watch": "NODE_ENV=development npm-watch"
  },
  "keywords": [
    "as3",
    "f5",
    "rest",
    "api"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@f5devcentral/eslint-config-f5-atg": "^0.1.1",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.25.0",
    "express": "^4.17.1",
    "marked": "^2.0.3",
    "mocha": "^8.3.0",
    "mock-fs": "^4.14.0",
    "nock": "^13.0.11",
    "node-polyfill-webpack-plugin": "^1.1.1",
    "npm-watch": "^0.9.0",
    "nyc": "^15.1.0",
    "redoc-cli": "^0.10.4",
    "sinon": "^10.0.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "eslintConfig": {
    "extends": "@f5devcentral/eslint-config-f5-atg",
    "rules": {
      "no-control-regex": "off"
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "nodejs/**/*.js",
      "lib/**/*.js"
    ]
  },
  "watch": {
    "buildgui": "presentation/app.js",
    "start": {
      "patterns": [
        "lib",
        "nodejs"
      ],
      "extensions": "js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@f5devcentral/atg-storage": "^1.1.0",
    "@f5devcentral/f5-fast-core": "git+https://github.com/f5devcentral/f5-fast-core.git#8564ecc3d0c4e895e6fa9b1098bd9c19cb8bdb5f",
    "@f5devcentral/f5-teem": "^1.5.0",
    "ajv": "^6.12.6",
    "axios": "^0.21.1",
    "core-js": "^3.12.0",
    "deepmerge": "^4.2.2",
    "extract-zip": "^1.7.0",
    "fs-extra": "^8.1.0",
    "jsonpath-plus": "^4.0.0",
    "mustache": "^4.2.0",
    "semver": "^7.3.5"
  }
}
