{
  "name": "f5-appsvcs-templates",
  "version": "1.14.0-rc.3",
  "author": "F5 Networks",
  "license": "Apache-2.0",
  "description": "",
  "repository": "github:F5Networks/f5-appsvcs-templates",
  "main": "nodejs/templateWorker.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue nodejs lib presentation/app.js presentation/pages presentation/components test scripts stand-alone",
    "test": "npm run testunit",
    "testunit": "mocha --recursive \"./test/unit/*.js\"",
    "testinteg": "mocha --recursive \"./test/integ/*.js\"",
    "testtemplates": "mocha --recursive \"./test/templates/*.js\"",
    "testall": "npm run lint && npm run testunit && npm run testtemplates",
    "coverage": "nyc npm test",
    "buildgui": "./scripts/build-presentation-layer.sh",
    "buildrpm": "./scripts/rpmbuild.sh",
    "build": "npm run buildgui && npm run buildrpm",
    "version": "./scripts/fix-version.sh",
    "start": "node --unhandled-rejections=strict stand-alone/app.js",
    "watch": "NODE_ENV=development npm-watch"
  },
  "keywords": [
    "as3",
    "f5",
    "rest",
    "api"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@f5devcentral/eslint-config-f5-atg": "^0.1.5",
    "@json-editor/json-editor": "^2.6.0",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^6.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^7.20.0",
    "express": "^4.17.1",
    "marked": "^2.1.3",
    "mocha": "^9.1.3",
    "nock": "^13.1.4",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "npm-watch": "^0.11.0",
    "nyc": "^15.1.0",
    "redoc-cli": "^0.10.4",
    "sinon": "^11.1.2",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "eslintConfig": {
    "extends": [
      "@f5devcentral/eslint-config-f5-atg",
      "plugin:vue/recommended"
    ],
    "rules": {
      "no-control-regex": "off",
      "strict": "off",
      "arrow-parens": [
        "error",
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "max-classes-per-file": "off",
      "vue/html-indent": [
        "error",
        4
      ]
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "nodejs/**/*.js",
      "lib/**/*.js"
    ]
  },
  "watch": {
    "buildgui": {
      "patterns": [
        "presentation/app.js",
        "presentation/pages/",
        "presentation/components/",
        "webpack.config.js"
      ],
      "extensions": [
        "js",
        "vue"
      ]
    },
    "start": {
      "patterns": [
        "lib",
        "nodejs",
        "templates",
        "stand-alone"
      ],
      "extensions": [
        "js",
        "json",
        "yaml"
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@f5devcentral/atg-shared-utilities": "^0.2.2",
    "@f5devcentral/f5-fast-core": "git+https://github.com/f5devcentral/f5-fast-core.git#ad7cef25b41a62eb2e539e2ea3a2d8c8f7110d09",
    "@f5devcentral/f5-teem": "^1.5.0",
    "ajv": "^6.12.6",
    "axios": "^0.21.2",
    "core-js": "^3.19.0",
    "deepmerge": "^4.2.2",
    "extract-zip": "^1.7.0",
    "fs-extra": "^8.1.0",
    "jsonpath-plus": "^4.0.0",
    "mustache": "^4.2.0",
    "semver": "^7.3.5"
  }
}
