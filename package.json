{
  "name": "f5-appsvcs-templates",
  "version": "1.20.0-rc.5",
  "author": "F5 Networks",
  "license": "Apache-2.0",
  "description": "",
  "repository": "github:F5Networks/f5-appsvcs-templates",
  "main": "nodejs/templateWorker.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue nodejs lib presentation/app.js presentation/pages presentation/components test scripts stand-alone",
    "test": "npm run testunit",
    "testunit": "mocha --recursive \"./test/unit/*.js\"",
    "testinteg": "mocha --recursive \"./test/integ/*.js\"",
    "testtemplates": "mocha --recursive \"./test/templates/*.js\"",
    "testall": "npm run lint && npm run testunit && npm run testtemplates",
    "test-mutation": "npx stryker run",
    "coverage": "nyc npm test",
    "buildgui": "./scripts/build-presentation-layer.sh",
    "buildrpm": "./scripts/rpmbuild.sh",
    "build": "npm run buildgui && npm run buildrpm",
    "buildperf": "F5_PERF_TRACING_ENABLED='true' npm run build",
    "version": "./scripts/fix-version.sh",
    "start": "node --unhandled-rejections=strict stand-alone/app.js",
    "watch": "NODE_ENV=development npm-watch"
  },
  "keywords": [
    "as3",
    "f5",
    "rest",
    "api"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@f5devcentral/eslint-config-f5-atg": "^0.1.7",
    "@json-editor/json-editor": "2.6.1",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/mocha-runner": "^5.6.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-openapi-response-validator": "^0.14.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^7.20.0",
    "express": "^4.18.1",
    "marked": "^4.0.16",
    "mocha": "^9.2.2",
    "mock-fs": "^5.1.2",
    "nock": "^13.2.4",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "npm-watch": "^0.11.0",
    "nyc": "^15.1.0",
    "react-is": "^17.0.2",
    "redoc-cli": "^0.13.14",
    "sinon": "^13.0.2",
    "vue": "^3.1.0",
    "vue-loader": "^16.8.3",
    "vue-style-loader": "^4.1.3",
    "vue-router": "^4.0.15",
    "@vue/compiler-sfc": "^3.1.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "extends": [
      "@f5devcentral/eslint-config-f5-atg",
      "plugin:vue/recommended"
    ],
    "rules": {
      "no-control-regex": "off",
      "strict": "off",
      "arrow-parens": [
        "error",
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "max-classes-per-file": "off",
      "vue/html-indent": [
        "error",
        4
      ]
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "stand-alone/expressAdapter.js",
      "nodejs/**/*.js",
      "lib/**/*.js"
    ]
  },
  "watch": {
    "buildgui": {
      "patterns": [
        "presentation/app.js",
        "presentation/pages/",
        "presentation/components/",
        "docs/openapi.yml",
        "webpack.config.js"
      ],
      "extensions": [
        "js",
        "vue"
      ]
    },
    "start": {
      "patterns": [
        "lib",
        "nodejs",
        "templates",
        "stand-alone"
      ],
      "extensions": [
        "js",
        "json",
        "yaml"
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@f5devcentral/atg-shared-utilities": "^0.4.5",
    "@f5devcentral/atg-storage": "^1.3.3",
    "@f5devcentral/f5-fast-core": "^0.19.1",
    "@f5devcentral/f5-teem": "^1.5.0",
    "ajv": "^6.12.6",
    "axios": "^0.27.2",
    "core-js": "^3.22.7",
    "deepmerge": "^4.2.2",
    "extract-zip": "^1.7.0",
    "fs-extra": "^8.1.0",
    "jaeger-client": "^3.19.0",
    "jsonpath-plus": "^4.0.0",
    "mustache": "^4.2.0",
    "semver": "^7.3.7",
    "uuid": "^7.0.3"
  },
  "optionalDependencies": {
    "error": "7.2.1"
  }
}
